{
  "name": "mtu-courses",
  "version": "0.1.2",
  "description": "üèõ scrape all courses for a given semester from Michigan Tech's website",
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "scripts": {
    "lint": "xo",
    "test": "npm run lint",
    "watch": "tsc --watch",
    "build": "tsc",
    "document": "typedoc --out docs src",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codetheweb/mtu-courses.git"
  },
  "bugs": {
    "url": "https://github.com/codetheweb/mtu-courses/issues"
  },
  "homepage": "https://github.com/codetheweb/mtu-courses#-mtu-courses",
  "keywords": [
    "üèõ",
    "mtu",
    "michigan tech",
    "course",
    "courses",
    "class",
    "schedule",
    "banweb",
    "student",
    "college",
    "scrape",
    "typescript"
  ],
  "author": "Max Isom <hi@maxisom.me> (https://maxisom.me/)",
  "license": "MIT",
  "devDependencies": {
    "@types/cheerio": "^0.22.13",
    "@types/got": "^9.6.7",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "eslint-config-xo-typescript": "^0.18.0",
    "husky": "^3.0.5",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.2",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "chrono-node": "^1.3.11",
    "got": "^9.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run build"
    }
  },
  "xo": {
    "space": true,
    "ignore": "docs",
    "extends": "xo-typescript",
    "rules": {
      "@typescript-eslint/indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ]
    },
    "extensions": [
      "ts"
    ]
  }
}
